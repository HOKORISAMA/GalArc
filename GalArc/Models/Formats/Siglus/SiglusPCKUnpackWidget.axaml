<UserControl x:Class="GalArc.Models.Formats.Siglus.SiglusPCKUnpackWidget"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:i="using:GalArc.I18n"
             xmlns:local="using:GalArc.Models.Formats.Siglus"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="450"
             d:DesignWidth="800"
             x:DataType="local:SiglusPCKUnpackOptions"
             mc:Ignorable="d">

    <Design.DataContext>
        <local:SiglusPCKUnpackOptions />
    </Design.DataContext>

    <StackPanel Spacing="10">
        <TextBlock Text="{x:Static i:GuiStrings.SelectEncScheme}" />
        <ComboBox HorizontalAlignment="Stretch"
                  ItemsSource="{Binding Names}"
                  SelectedItem="{Binding SelectedName, Mode=TwoWay}" />
    </StackPanel>
</UserControl>
