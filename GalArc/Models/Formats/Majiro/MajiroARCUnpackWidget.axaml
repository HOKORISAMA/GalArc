<UserControl x:Class="GalArc.Models.Formats.Majiro.MajiroARCUnpackWidget"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:i="using:GalArc.I18n"
             xmlns:local="using:GalArc.Models.Formats.Majiro"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="450"
             d:DesignWidth="800"
             x:DataType="local:MajiroARCUnpackOptions"
             mc:Ignorable="d">

    <Design.DataContext>
        <local:MajiroARCUnpackOptions />
    </Design.DataContext>

    <StackPanel Spacing="10">
        <CheckBox Content="{x:Static i:GuiStrings.DecryptScripts}" IsChecked="{Binding DecryptScripts}" />
    </StackPanel>
</UserControl>
